{
  "name": "gist-blog",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "gist-blog",
  "author": "toms74209200",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "test": "vitest run --silent",
    "test:small": "vitest run --silent --config vite.small.config.ts",
    "test:medium": "vitest run --silent --config vite.medium.config.ts",
    "dev": "react-router dev",
    "build": "react-router build && cp -r build/client/assets build/client/static build/client/${PATH_PREFIX:-}/",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc --noEmit"
  },
  "dependencies": {
    "@react-router/node": "^7.11.0",
    "@react-router/serve": "^7.11.0",
    "@tailwindcss/postcss": "^4.0.4",
    "isbot": "^5",
    "postcss": "^8.4.49",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-helmet-async": "^2.0.5",
    "react-markdown": "^10.1.0",
    "react-router": "^7.11.0",
    "rehype-raw": "^7.0.0",
    "remark-gemoji": "^8.0.0",
    "remark-gfm": "^4.0.0",
    "satori": "^0.18.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.0.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.11.0",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "typescript": "^5.9.2",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^3.0.5"
  }
}
